{
  "name": "nodejs-pro-",
  "version": "1.0.0",
  "description": "learn nodejs",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesHarrision/learn-nodejs-helloworld.git"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "jack1e",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesHarrision/learn-nodejs-helloworld/issues"
  },
  "homepage": "https://github.com/JamesHarrision/learn-nodejs-helloworld#readme",
  "dependencies": {
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.7",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
